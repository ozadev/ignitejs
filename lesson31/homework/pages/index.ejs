<body> 
	<div class="panel panel-primary">
        <table class="table table-striped">
			<thead>
				<tr>
					<th>Name</th>
					<th>Info</th>
				</tr>
			</thead>
			<tbody>
				<% data.forEach(function(item) { %>
				<tr>
					<td><%= item.name %></td>
					<td><%= item.info %></td>
				</tr>
				<% }); %>
			</tbody>
		</table>
	</div>
	<a class="btn btn-default" href="/addItem">Add Item</a>
</body>
</html>